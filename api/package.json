{
  "name": "jui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "NODE_ENV=production webpack --config ./webpack-node.config.js",
    "coverage": "npx nyc mocha -r dotenv-extended/config",
    "lint": "npx tslint --project .",
    "watch": "nodemon --watch '**/*.ts' --exec ts-node -r dotenv-extended/config local.ts",
    "test": "mocha -r dotenv-extended/config",
    "test:watch": "mocha -r dotenv-extended/config --watch --recursive --watch-extensions ts",
    "postinstall": "cd .."
  },
  "private": true,
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "html",
      "text-summary"
    ],
    "require": [],
    "cache": true,
    "all": true,
    "report-dir": "../reports/tests/coverage/node"
  },
  "dependencies": {
    "@hmcts/nodejs-healthcheck": "^1.7.0",
    "@hmcts/properties-volume": "^0.0.13",
    "@hmcts/rpx-xui-node-lib": "^2.24.4",
    "applicationinsights": "^1.5.0",
    "axios": "^0.21.1",
    "config": "^3.2.4",
    "cookie-parser": "^1.4.4",
    "ejs": "^2.6.1",
    "exception-formatter": "^1.0.7",
    "express": "^4.17.3",
    "form-data": "^3.0.0",
    "global-agent": "^2.1.8",
    "helmet": "^3.21.3",
    "log4js": "^6.4.4",
    "multer": "^1.4.2",
    "otp": "^0.1.3",
    "p-iteration": "^1.1.7",
    "@nguniversal/express-engine": "^7.1.1",
    "@types/express-session": "^1.17.0",
    "@types/passport": "^1.0.3",
    "express-session": "^1.17.2",
    "connect-redis": "^4.0.4",
    "openid-client": "^3.14.2",
    "passport": "^0.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/global-agent": "^2.1.0",
    "@types/node": "16.11.2",
    "codelyzer": "~4.2.1",
    "dotenv-extended": "^2.7.1",
    "mocha": "^5.2.0",
    "mochawesome": "^4.1.0",
    "nodemon": "^1.18.9",
    "ts-loader": "^6.2.1",
    "ts-node": "^5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~6.1.3",
    "typescript": "4.4.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
}
