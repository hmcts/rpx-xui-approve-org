{
  "name": "jui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "NODE_ENV=production webpack --config ./webpack-node.config.js",
    "coverage": "npx nyc mocha -r dotenv-extended/config",
    "lint": "npx tslint --project .",
    "watch": "nodemon --watch '**/*.ts' --exec ts-node -r dotenv-extended/config local.ts",
    "test": "mocha -r dotenv-extended/config",
    "test:watch": "mocha -r dotenv-extended/config --watch --recursive --watch-extensions ts"
  },
  "private": true,
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "html",
      "text-summary"
    ],
    "require": [],
    "cache": true,
    "all": true,
    "report-dir": "../reports/tests/coverage/node"
  },
  "dependencies": {
    "@hmcts/nodejs-healthcheck": "^1.7.0",
    "@hmcts/properties-volume": "^0.0.13",
    "@hmcts/rpx-xui-node-lib": "^2.24.4",
    "applicationinsights": "^1.5.0",
    "axios": "^0.21.1",
    "config": "^3.2.4",
    "cookie-parser": "^1.4.4",
    "ejs": "^2.6.1",
    "exception-formatter": "^1.0.7",
    "express": "^4.17.3",
    "form-data": "^3.0.0",
    "global-agent": "^2.1.8",
    "helmet": "^3.21.3",
    "log4js": "^3.0.6",
    "multer": "^1.4.2",
    "otp": "^0.1.3",
    "p-iteration": "^1.1.7",
    "@nguniversal/express-engine": "^7.1.1",
    "@types/express-session": "^1.17.0",
    "@types/passport": "^1.0.3",
    "express-session": "^1.17.2",
    "connect-redis": "^4.0.4",
    "openid-client": "^3.14.2",
    "passport": "^0.4.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^7.2.13",
    "@angular/language-service": "~7.2.0",
    "@pact-foundation/pact": "^9.2.0",
    "@pact-foundation/pact-node": "^9.0.6",
    "@stryker-mutator/api": "^3.0.1",
    "@stryker-mutator/core": "^3.0.1",
    "@stryker-mutator/html-reporter": "^3.0.1",
