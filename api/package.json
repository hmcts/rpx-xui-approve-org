    "mochawesome": "3.1.1",
    "ng-packagr": "^3.0.0-rc.2",
    "ngrx-store-freeze": "^0.2.4",
    "nodemon": "^1.18.9",
    "pa11y": "^5.3.0",
    "protractor": "~5.4.0",
