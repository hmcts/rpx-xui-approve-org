{
    "/api/*": {
        "_comment": "This is where we setup Webpacks proxy, to hit our non-secure content on port 3001.",
        "target": "https://localhost:3001",
        "secure": false,
        "logLevel": "debug"
    },
    "/oauth2/callback": {
        "target": "https://localhost:3001",
        "secure": false,
        "logLevel": "debug"
    }
}
